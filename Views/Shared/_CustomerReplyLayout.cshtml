                    <tr>
                        <td>
                            <!-- logo -->
                            <table width="100%" align="center" border="0" cellpadding="0" cellspacing="0" class="devicewidth">
                                <tbody>
                                    <tr>
                                        <td width="140" height="60" align="center">
                                            <div class="imgpop">

                                                <img src="http://itsutools.itsucomms.com/Public/images/2017/02/0001/logo.jpg" alt="" border="0" style="display:block; border:none; outline:none; text-decoration:none;">

                                            </div>
                                        </td>
                                    </tr>
                                </tbody>
                            </table>
                            <!-- end of logo -->

                        </td>
                    </tr>
                    <!-- Spacing -->
                    <tr>
                        <td bgcolor="#e53e96" height="5" style="font-size:1px; line-height:1px; ">&nbsp;</td>
                    </tr>
                    <!-- Spacing -->
                </tbody>
            </table>
        </div>
    }
    else if (Client.ToLower() == "bills" || Client.ToLower() == "bills_uat")
    {
        <div>
            <table bgcolor="#ffffff" width="100%" cellpadding="0" cellspacing="0" border="0" align="center">
